{
  "elements": [
    {
      "type": "Label",
      "label": "This device type is not supported"
    },
    {
      "type": "Button",
      "label": "Start remote pair",
      "onClick": "DUOFERN_WriteValueBoolean($id, 'REMOTE_PAIR', true)"
    },
    {
      "type": "Button",
      "label": "Stop remote pair/unpair",
      "onClick": "DUOFERN_WriteValueBoolean($id, 'REMOTE_PAIR', false)"
    },
    {
      "type": "Button",
      "label": "Start remote unpair",
      "onClick": "DUOFERN_WriteValueBoolean($id, 'REMOTE_UNPAIR', true)"
    },
    {
      "type": "Button",
      "label": "Reset",
      "onClick": "DUOFERN_WriteValueBoolean($id, 'RESET_DEVICE', true)"
    },
    {
      "type": "Button",
      "label": "Full reset",
      "onClick": "DUOFERN_WriteValueBoolean($id, 'FULL_RESET_DEVICE', true)"
    },
    {
      "type": "Label",
      "label": "\n"
    },
    {
      "type": "Label",
      "label": "Product name: %s"
    },
    {
      "type": "ValidationTextBox",
      "name": "duoFernCode",
      "caption": "DuoFern Code"
    },
    {
      "type": "Label",
      "label": "Version: %s"
    }
  ],
  "status": [
    {
      "code": 101,
      "icon": "inactive",
      "caption": "Instance will be created"
    },
    {
      "code": 102,
      "icon": "active",
      "caption": "Instance is active"
    },
    {
      "code": 103,
      "icon": "inactive",
      "caption": "Instance will be deleted"
    },
    {
      "code": 104,
      "icon": "inactive",
      "caption": "Instance is inactive"
    },
    {
      "code": 201,
      "icon": "error",
      "caption": "Invalid DuoFern code (Format: XXXXXX with X = 0-9, A-F)"
    },
    {
      "code": 202,
      "icon": "error",
      "caption": "Device is not available"
    }
  ]
}